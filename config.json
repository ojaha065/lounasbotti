{
	"$schema": "https://jhaiko.tech/lounasbotti.schema.json",
	"instanceId": "changeThis",
	"dataProvider": "ruokapaikkaFi",
	"triggerRegExp": "!(lounas|ruokaa|nälkä)",
	"defaultRestaurants": ["savo", "talli", "rami", "august"],
	"additionalRestaurants": ["holvi", "vino", "fernando", "pormestari", "lale", "ramiVisulahti"],
	"customErrorMessages": [
		["savo", "Katso lounaslista osoitteesta https://www.vaiha.fi/savo-lounas"],
		["talli", "Katso lounaslista osoitteesta https://www.xamkravintolat.fi/tallin-lounaslista/"],
		["rami", "Katso lounaslista osoitteesta https://www.raminkonditoria.fi/lounaslistat/"],
		["august", "Katso lounaslista osoitteesta https://www.ravintolaaugust.fi/ (sivun alalaita)"],
		["holvi", "Katso lounaslista osoitteesta https://www.bistroholvi.fi/#lounas"],
		["vino", "Katso lounaslista osoitteesta https://www.ravintolavino.fi/lounas"],
		["pormestari", "Katso lounaslista osoitteesta https://www.vaiha.fi/lounas-pormestari"],
		["lale", "Tätä lounaslistaa ei juuri nyt ole saatavissa verkossa. Huomaa, että Ravintola Lale on suljettu maanantaisin, jolloin tämä virhe on odotettu."],
		["ramiVisulahti", "Katso lounaslista osoitteesta https://www.raminkonditoria.fi/lounaslistat/"]
	],
	"gitUrl": "https://github.com/ojaha065/lounasbotti",
	"displayVoters": true,
	"iconsEnabled": true,
	"adminUsers": [],
	"emojiRules": [
		["(?<!kur)pi(?:zz|ts)a", ":pizza:"],
		["tortilla|burrito", ":burrito:"],
		["joulu|rosolli|glögi", ":christmas_tree:"],
		["keitto", ":bowl_with_spoon:"],
		["((?<!pork)kana(?!nmun))|(broiler)", ":chicken:"],
		["(?:(?<!pa)loh(i|ta){1})|((?<!rans)kala(?!jitelma))|(mui(kut|kkuja){1})|sei(ti|tä)|(?:kampela)|(?:turska)|(?:hauk(i|ea))", ":fish:"],
		["(?:liha(?:pul|pyöry))|falafel", ":falafel:"],
		["(?:po(?:rsa|ss))|(?:si(?:an|ka))", ":pig2:"],
		["(?:pekoni)|(?:bacon)", ":bacon:"],
		["(?:spagetti)|(?:bolognese)", ":spaghetti:"],
		["härk?ä", ":ox:"],
		["riista", ":deer:"],
		["nau(?:dan|ta)", ":cow:"],
		["pannukak", ":pancakes:"],
		["riisi", ":rice:"],
		["chips|ranskalai(?:set|sia)|ranskanperu", ":fries:"],
		["porkkan", ":carrot:"],
		["sieni", ":mushroom:"],
		["(?:kasvi(?:s|k))|(?:juure)", ":tomato:"],
		["salaat{1,2}(?:i|eja)", ":green_salad:"],
		["jälkiru(?:u|o)|leipurin\\s*mak|vispipuur|vanuk|kiisseli|vaah(?:toa|don)|mous{1,2}e|rahkaa|pannacot|trif{1,2}le|smoothie|rita(?:rit|reita)|kakku|leivos|tuulihattu|let(?:ut|tuja)|raparperi|pitko|jäätelö|piirakka|hyvää", ":yum:"],
		["(?<!(?:kasvis)|(?:juur(?:i|(?:es))))pihv", ":cut_of_meat:"],
		["ohukai", ":yum:"],
		["paprik", ":bell_pepper:"],
		["peru(?:na|no)", ":potato:"],
		["kahvi", ":coffee:"],
		["appelsiini", ":tangerine:"],
		["soija|veg(?:a|\\.)", ":seedling:"],
		["mummo|isoäi", ":older_woman:"],
		["suljettu|kiinni", ":no_entry_sign:"],
		["kaalikääry", ":nauseated_face:"]
	]
}